{
  "name": "mcan.sh",
  "version": 2,
  "alias": [
    "mcansh.now.sh",
    "loganmcansh.com"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "static": {
    "directoryListing": false,
    "headers": [
      {
        "source": "**/static/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }
}
