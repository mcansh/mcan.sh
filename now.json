{
  "name": "mcan.sh",
  "version": 2,
  "alias": [
    "mcansh.now.sh",
    "loganmcansh.com"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "**/static/**",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
