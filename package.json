{
  "name": "mcansh",
  "description": "personal website for logan mcansh",
  "version": "1.0.14",
  "repository": "mcansh/mcan.sh",
  "homepage": "https://mcan.sh",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "prettier": "eslint-config-mcansh/prettier.config.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint . --ext \".js,.ts,.tsx\"",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "validate": "yarn lint && yarn format && yarn type-check"
  },
  "dependencies": {
    "@mcansh/custom-next-link": "0.1.0",
    "next": "9.0.7",
    "polished": "3.4.1",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-simple-img": "2.3.6",
    "styled-components": "4.4.0"
  },
  "devDependencies": {
    "@types/node": "12.7.11",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/styled-components": "4.1.19",
    "@typescript-eslint/eslint-plugin": "2.3.2",
    "@typescript-eslint/parser": "2.3.2",
    "babel-plugin-polished": "1.1.0",
    "babel-plugin-root-import": "6.4.1",
    "babel-plugin-styled-components": "1.10.6",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-kentcdodds": "14.4.1",
    "eslint-config-mcansh": "2.2.0-canary.0",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.15.1",
    "eslint-plugin-react-hooks": "2.1.2",
    "husky": "3.0.8",
    "lint-staged": "9.4.1",
    "prettier": "1.18.2",
    "typescript": "3.7.0-beta"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
      "sh scripts/ts-precommit.bash",
      "prettier --write",
      "git add"
    ]
  }
}
