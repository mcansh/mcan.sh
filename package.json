{
  "name": "mcansh",
  "description": "personal website for logan mcansh",
  "version": "1.0.14",
  "repository": "mcansh/mcan.sh",
  "homepage": "https://mcan.sh",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint . --ext \".js,.ts,.tsx\"",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "validate": "yarn lint && yarn format && yarn type-check"
  },
  "dependencies": {
    "@mcansh/custom-next-link": "0.3.2",
    "framer-motion": "1.6.15",
    "next": "9.1.4",
    "polished": "3.4.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-simple-img": "2.3.7",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@mcansh/eslint-config": "2.3.1-canary.0",
    "@types/node": "12.12.11",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/styled-components": "4.4.0",
    "@typescript-eslint/eslint-plugin": "2.8.0",
    "@typescript-eslint/parser": "2.8.0",
    "babel-plugin-polished": "1.1.0",
    "babel-plugin-root-import": "6.4.1",
    "babel-plugin-styled-components": "1.10.6",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-kentcdodds": "14.6.1",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.8.0",
    "husky": "3.1.0",
    "lint-staged": "9.4.3",
    "prettier": "1.19.1",
    "typescript": "3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
      "sh scripts/ts-precommit.bash",
      "prettier --write",
      "git add"
    ]
  }
}
