{
  "name": "mcansh",
  "description": "personal website for logan mcansh",
  "version": "2.0.0",
  "repository": "mcansh/mcan.sh",
  "homepage": "https://mcan.sh",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint . --ext \".js,.ts,.tsx\"",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "validate": "yarn lint && yarn format && yarn type-check"
  },
  "dependencies": {
    "@mcansh/custom-next-link": "0.3.2",
    "framer-motion": "2.9.1",
    "next": "9.5.5",
    "polished": "4.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-simple-img": "2.3.9",
    "styled-components": "5.2.0"
  },
  "devDependencies": {
    "@mcansh/eslint-config": "2.8.0",
    "@svgr/webpack": "5.4.0",
    "@types/node": "14.11.8",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "babel-plugin-polished": "1.1.0",
    "babel-plugin-styled-components": "1.11.1",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-kentcdodds": "16.1.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.10.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "sh scripts/ts-precommit.bash",
      "prettier --write",
      "eslint --fix"
    ],
    "*.+(json|yml|yaml|css|less|scss|md|graphql|mdx)": [
      "prettier --write"
    ]
  }
}
