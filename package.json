		"husky": "^9.1.7",
		"knip": "^5.38.3",
		"lightningcss": "^1.28.2",
		"lint-staged": "^15.2.10",
		"micromatch": "^4.0.8",
		"prettier": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"tailwindcss": "4.0.0-beta.2",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0",
		"vite": "^6.0.1"
